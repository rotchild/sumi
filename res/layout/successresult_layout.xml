<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
  <LinearLayout
     android:layout_width="match_parent"
     android:layout_height="45dp"
     android:orientation="horizontal"
     android:background="@drawable/myorderback"
     
     >
   <LinearLayout
    android:layout_width="3dp"
    android:layout_height="match_parent"
    android:orientation="horizontal"
    android:layout_weight="1"
    >   
</LinearLayout>
<LinearLayout
    android:layout_width="3dp"
    android:layout_height="match_parent"
    android:orientation="horizontal"
    android:layout_weight="1"
    >
   <TextView
       android:layout_margin="10dp"
       android:layout_width="match_parent"
       android:layout_height="match_parent"
       android:text="@string/fuelservice"
       android:textSize="@dimen/mordertitle_size"
       android:textColor="@color/white"
       android:gravity="center"
       /> 
</LinearLayout>
<LinearLayout
    android:layout_width="3dp"
    android:layout_height="match_parent"
    android:orientation="horizontal"
    android:layout_weight="1"
    >
 
</LinearLayout>      
 </LinearLayout>
 <LinearLayout
     android:layout_width="match_parent"
     android:layout_height="3dp"
     android:layout_weight="1"
     android:orientation="vertical"
     android:background="@color/order_background"
     >
 <ScrollView
     android:layout_width="match_parent"
     android:layout_height="wrap_content"
     android:fadeScrollbars="true"
     >
  <LinearLayout
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:orientation="vertical"
      >
    <LinearLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal"
    android:background="@color/white"
    >
 <RelativeLayout
     android:layout_width="match_parent"
     android:layout_height="wrap_content"
     >
  <ImageView
      android:id="@+id/success_icon"
      android:layout_width="60dp"
      android:layout_height="60dp"
      android:scaleType="centerInside"
      android:src="@drawable/success"
      android:layout_centerHorizontal="true"
      android:layout_marginTop="5dp"
      />
  <TextView
      android:id="@+id/success_label"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_below="@id/success_icon"
      android:text=""
      android:textSize="15dp"
      android:textColor="@color/black"
      android:layout_centerHorizontal="true"
      android:layout_marginTop="5dp"
      android:padding="5dp"
      />   
 </RelativeLayout>  
</LinearLayout>
<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="5dp"
    android:background="@color/order_background"
    >  
</LinearLayout>
<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:background="@color/white"
    >
 <RelativeLayout
     android:layout_width="match_parent"
     android:layout_height="@dimen/resultInfoItemHeight"
     >
  <TextView
      android:layout_height="match_parent"
      android:layout_width="wrap_content"
      android:text="订单号:"
      android:textSize="@dimen/resultInfo"
      android:textColor="@color/gray"
      android:gravity="center"
      android:padding="5dp"
      />
   <TextView
      android:id="@+id/ordernumVal"
      android:layout_height="match_parent"
      android:layout_width="wrap_content"
      android:text=""
      android:textSize="@dimen/resultInfo"
      android:textColor="@color/mdarkgray"
      android:gravity="center"
      android:padding="5dp"
      android:layout_alignParentRight="true"
      />   
 </RelativeLayout>
 
  <RelativeLayout
     android:layout_width="match_parent"
     android:layout_height="@dimen/resultInfoItemHeight"
     >
  <TextView
      android:layout_height="match_parent"
      android:layout_width="wrap_content"
      android:text="创建时间："
      android:textSize="@dimen/resultInfo"
      android:textColor="@color/gray"
      android:gravity="center"
      android:padding="5dp"
      />
   <TextView
      android:id="@+id/createTimeVal"
      android:layout_height="match_parent"
      android:layout_width="wrap_content"
      android:text=""
      android:textSize="@dimen/resultInfo"
      android:textColor="@color/mdarkgray"
      android:gravity="center"
      android:padding="5dp"
      android:layout_alignParentRight="true"
      />   
 </RelativeLayout>
 
   <RelativeLayout
     android:layout_width="match_parent"
     android:layout_height="@dimen/resultInfoItemHeight"
     >
  <TextView
      android:layout_height="match_parent"
      android:layout_width="wrap_content"
      android:text="交易商户"
      android:textSize="@dimen/resultInfo"
      android:textColor="@color/gray"
      android:gravity="center"
      android:padding="5dp"
      />
   <TextView
      android:id="@+id/sellerVal"
      android:layout_height="match_parent"
      android:layout_width="wrap_content"
      android:text=""
      android:textSize="@dimen/resultInfo"
      android:textColor="@color/mdarkgray"
      android:gravity="center"
      android:padding="5dp"
      android:layout_alignParentRight="true"
      />   
 </RelativeLayout>
 
    <RelativeLayout
     android:layout_width="match_parent"
     android:layout_height="@dimen/resultInfoItemHeight"
     >
  <TextView
      android:layout_height="match_parent"
      android:layout_width="wrap_content"
      android:text="加油金额："
      android:textSize="@dimen/resultInfo"
      android:textColor="@color/gray"
      android:gravity="center"
      android:padding="5dp"
      />
   <TextView
      android:id="@+id/fuelMoneyVal"
      android:layout_height="match_parent"
      android:layout_width="wrap_content"
      android:text=""
      android:textSize="@dimen/resultInfo"
      android:textColor="@color/mdarkgray"
      android:gravity="center"
      android:padding="5dp"
      android:layout_alignParentRight="true"
      />   
 </RelativeLayout>
 
     <RelativeLayout
     android:layout_width="match_parent"
     android:layout_height="@dimen/resultInfoItemHeight"
     >
  <TextView
      android:layout_height="match_parent"
      android:layout_width="wrap_content"
      android:text="支付方式:"
      android:textSize="@dimen/resultInfo"
      android:textColor="@color/gray"
      android:gravity="center"
      android:padding="5dp"
      />
   <TextView
      android:id="@+id/payMethodVal"
      android:layout_height="match_parent"
      android:layout_width="wrap_content"
      android:text=""
      android:textSize="@dimen/resultInfo"
      android:textColor="@color/mdarkgray"
      android:gravity="center"
      android:padding="5dp"
      android:layout_alignParentRight="true"
      />   
 </RelativeLayout>
 
      <RelativeLayout
     android:layout_width="match_parent"
     android:layout_height="@dimen/resultInfoItemHeight"
     >
  <TextView
      android:layout_height="match_parent"
      android:layout_width="wrap_content"
      android:text="会员积分:"
      android:textSize="@dimen/resultInfo"
      android:textColor="@color/gray"
      android:gravity="center"
      android:padding="5dp"
      />
   <TextView
      android:id="@+id/memberPointVal"
      android:layout_height="match_parent"
      android:layout_width="wrap_content"
      android:text=""
      android:textSize="@dimen/resultInfo"
      android:textColor="@color/mdarkgray"
      android:gravity="center"
      android:padding="5dp"
      android:layout_alignParentRight="true"
      />   
 </RelativeLayout>
 
       <RelativeLayout
     android:layout_width="match_parent"
     android:layout_height="@dimen/resultInfoItemHeight"
     >
  <TextView
      android:layout_height="match_parent"
      android:layout_width="wrap_content"
      android:text="代金券:"
      android:textSize="@dimen/resultInfo"
      android:textColor="@color/gray"
      android:gravity="center"
      android:padding="5dp"
      />
   <TextView
      android:id="@+id/vouchesVal"
      android:layout_height="match_parent"
      android:layout_width="wrap_content"
      android:text=""
      android:textSize="@dimen/resultInfo"
      android:textColor="@color/mdarkgray"
      android:gravity="center"
      android:padding="5dp"
      android:layout_alignParentRight="true"
      />   
 </RelativeLayout>
 
        <RelativeLayout
     android:layout_width="match_parent"
     android:layout_height="@dimen/resultInfoItemHeight"
     >
  <TextView
      android:layout_height="match_parent"
      android:layout_width="wrap_content"
      android:text="实付金额:"
      android:textSize="@dimen/resultInfo"
      android:textColor="@color/gray"
      android:gravity="center"
      android:padding="5dp"
      />
   <TextView
      android:id="@+id/realPayVal"
      android:layout_height="match_parent"
      android:layout_width="wrap_content"
      android:text=""
      android:textSize="@dimen/resultInfo"
      android:textColor="@color/mdarkgray"
      android:gravity="center"
      android:padding="5dp"
      android:layout_alignParentRight="true"
      />   
 </RelativeLayout>
</LinearLayout>
<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:layout_marginTop="5dp"
    >
  <Button
      android:id="@+id/success_finish"
      android:layout_width="match_parent"
      android:layout_height="33dp"
      android:layout_margin="5dp"
      android:text="完成"
      android:textSize="15dp"
      android:textColor="@color/white"
      android:background="@drawable/bulebuttonbackground"
      />
    <Button
      android:id="@+id/reprint"
      android:layout_width="match_parent"
      android:layout_height="33dp"
      android:layout_margin="5dp"
      android:text="再次打印小票"
      android:textSize="15dp"
      android:textColor="@color/white"
      android:background="@drawable/bulebuttonbackground"
      />   
</LinearLayout>  
  </LinearLayout>   
 </ScrollView>    
     
 </LinearLayout>   
</LinearLayout>
